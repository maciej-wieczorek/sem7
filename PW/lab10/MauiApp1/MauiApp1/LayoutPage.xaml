<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.LayoutPage"
             Title="LayoutPage">
	<FlexLayout Direction="Column" Wrap="Wrap">
		<Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
		<Button Text="My Bytton" />
		<BoxView Margin="10" WidthRequest="100" HeightRequest="100" Color="Red"></BoxView>
		<Label Text="Label" HorizontalOptions="Center"></Label>
	</FlexLayout>
</ContentPage>